<!--pages/components/swipe/swipe.wxml-->
<view>
	<swiper style="width:100%;height:450rpx" class="swiper" indicator-dots="false" autoplay="true" interval="3000" duration="500" circular="true">
		<block wx:for="{{imageSrc}}" wx:key="*this">
			<swiper-item>
				<van-image width="100%" height="100%" fit="cover" src="{{item}}" />
			</swiper-item>
		</block>
	</swiper>
</view>